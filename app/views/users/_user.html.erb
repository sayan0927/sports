<div id="<%= dom_id user %>">
    <p>
        <strong>Username:</strong>
        <%= user.username %>
    </p>

    <p>
        <strong>User Type:</strong>
        <%= user.usertype %>
    </p>

    <p>
        <strong>Full Name:</strong>
        <%= user.fullname %>
    </p>

    <p>
        <strong>Roll No:</strong>
        <%= user.roll %>
    </p>

    <p>
        <strong>Phone:</strong>
        <%= user.phone %>
    </p>


</div>
